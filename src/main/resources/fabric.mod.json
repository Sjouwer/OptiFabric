{
  "schemaVersion": 1,
  "id": "optifabric-origins",
  "provides": [
    "optifabric"
  ],
  "name": "OptiFabric Origins",
  "version": "${version}",
  "description": "Optifabric is a mod that loads optifine into fabric",
  "authors": [
    "Modmuss50",
    "Chocohead"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/optifabric-origins",
    "sources": "https://github.com/Sjouwer/OptiFabric-Origins",
    "issues": "https://github.com/Sjouwer/OptiFabric-Origins/issues"
  },
  "mixins" :[
    "optifabric.mixins.json"
  ],
  "license": "MPL-2.0",
  "icon": "assets/optifabric/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "me.modmuss50.optifabric.mod.Optifabric"
    ],
    "mm:early_risers": [
      "me.modmuss50.optifabric.mod.OptifabricSetup"
    ]
  },
  "custom": {
    "fabric-renderer-api-v1:contains_renderer": true
  },
  "depends": {
    "fabricloader": ">=0.15.0"
  }
}
